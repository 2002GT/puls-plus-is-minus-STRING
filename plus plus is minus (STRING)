#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t; cin>>t;
    while(t--)
    {
        string s,t;cin>>s>>t; int flag=0;
        if(s!=t)
        {
            if(s.length()<=t.length())
            {
                flag=1;
            }
            else
            {
                int j=0;
                for(int i=0 ; i<s.length() ; i++)
                {
                    if(s[i]==t[j]) j++;
                    else if(t[j]!='-'&&s[i]=='-'&&s[i+1]=='-')
                    {
                        j++;i++;
                    }
                    else{
                        flag=1; break;
                    }
                }
            }
        }
        if(flag==1) cout<<"NO"<<endl;
        else cout<<"YES"<<endl;
    }
    return 0;
}

